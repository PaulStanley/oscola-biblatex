\newcommand\paratextformatted[1]{%
  \ifnumeral{#1}
  {\bibstring{paragraph}\addspace\printtext{#1}}
  {\ifnumerals{#1}
    {\bibstring{paragraphs}\addspace\printtext{#1}}
    {\printtext{#1}}}}

